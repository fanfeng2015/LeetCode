package compareVersionNumbers;

public class CompareVersionNumbers {

	public int compareVersion(String version1, String version2) {
		int i = 0, j = 0, m = 0, n = 0;
		while ((m = version1.indexOf('.', i)) != -1 || (n = version2.indexOf('.', j)) != -1) {
			int a = m == -1 ? 0 : Integer.parseInt(version1.substring(i, m));
			int b = n == -1 ? 0 : Integer.parseInt(version2.substring(j, n));
			if (a > b) {
				return 1;
			} else if (a < b) {
				return -1;
			}
			i = m + 1;
			j = n + 1;
		}
		return 0;
	}

}
