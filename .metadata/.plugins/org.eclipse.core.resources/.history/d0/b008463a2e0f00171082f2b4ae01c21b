// LeetCode #259

public class ThreeSumSmaller {

	private static int count = 0;

	public int threeSumSmaller(int[] nums, int target) {
		Arrays.sort(nums);
		for (int i = 0; i < nums.length; i++) {
			twoSumSmaller(nums, target - nums[i], i + 1);
		}
		return count;
	}

	private void twoSumSmaller(int[] nums, int target, int begin) {

	}

}
