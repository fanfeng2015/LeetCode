package decodeWays;

// LeetCode #91

public class DecodeWays {

	public int numDecodings(String s) {
		char[] charArray = s.toCharArray();
		return numDecodings(charArray, 0, charArray.length - 1);
	}

	private int numDecodings(char[] charArray, int left, int right) {
		// base case
		if (left == right) {
			return 1;
		} else if (left == right - 1) {
			int val = 10 * Character.getNumericValue(charArray[left]) + Character.getNumericValue(charArray[right]);
			return 
		}
	}

}
