package compareVersionNumbers;

public class CompareVersionNumbers {

	public int compareVersion(String version1, String version2) {
		int i = version1.indexOf('.'), j = version2.indexOf('.');
		int a = Integer.parseInt(version1.substring(0, i));
		int b = Integer.parseInt(version2.substring(0, j));
		if (a > b) {
			return 1;
		} else if (a < b) {
			return -1;
		}
		// a == b

	}

}
