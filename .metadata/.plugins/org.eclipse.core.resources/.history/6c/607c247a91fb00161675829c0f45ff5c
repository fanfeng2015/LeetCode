package gameOfLife;

// LeetCode #289

public class GameOfLife {

	public void gameOfLife(int[][] board) {
		int[][] M = copy(board);
	}
	
	private int[][] copy(int[][] board) {
		int m = board.length, n = board[0].length;
		int[][] result = new int[m][n];
		for (int i = 0; i < m; i++) {
			for (int j = 0; j < n; j++) {
				result[i][j] = board[i][j];
			}
		}
		return result;
	}

}
