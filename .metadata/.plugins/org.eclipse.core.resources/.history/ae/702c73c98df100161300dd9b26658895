package kthSmallestInBST;

public int kthSmallest(TreeNode root,int k){LinkedList<TreeNode>stack=new LinkedList<>();stack.offerLast(root);TreeNode cur=root;while(cur!=null||stack.size()!=0){if(cur!=null){stack.offerLast(cur);cur=cur.left;}else{cur=stack.pollLast();k--;if(k==0){return cur.val;}cur=cur.right;}}return Integer.MIN_VALUE;}
